#include <stdio.h>

int main() {
  int nota;

printf("Introduzca la nota del alumno de 1 a 10:");
  scanf(" %d",&nota);

  if (nota<5){
    printf("El alumno ha suspendido la clase ");
  }
  else if (nota == 5){
    printf("El alumno ha sacado un suficiente");
  }
  else if (nota == 6){
    printf( "El alumno ha sacado un bien");
  }
  else if (nota == 7 || nota == 8){
    printf("El alumno ha sacado un notable");
  }
  else if (nota == 9 || nota == 10){
    printf("El alumno es sobresaliente");
  }
  else{
    printf("La nota introducida no es valida");
  }

}
