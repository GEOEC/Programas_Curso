#include <stdio.h>

int main() {
  int vector[10] = {8 , 6 , -5 , 7 , -30 , 8 , 9 , -2 , 5 , 7};
  int max = vector[0];
  int min = vector[0];

  for (int i=0; i<10; i++) {
    if (vector[i] > max) {
      max = vector[i];
  }
    if (vector[i] < min) {
      min = vector[i];
  }
}
  printf("Valor minimo: %d \nValor maximo: %d", min, max);

  
}
